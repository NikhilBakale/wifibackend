# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Flask Configuration
FLASK_ENV=production
PORT=8000

# Google Drive API Credentials (JSON format)
# Get these from Google Cloud Console
CLIENT_SECRETS_JSON={"installed":{"client_id":"your-client-id","project_id":"your-project","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_secret":"your-client-secret","redirect_uris":["http://localhost"]}}

# Google Drive Credentials (After first authentication)
CREDENTIALS_JSON={"access_token":"your-tokens-here"}
